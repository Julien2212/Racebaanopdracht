<Window x:Class="WpfApp.StatsParticipantsCompetition"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="StatsParticipantsCompetition" Height="450" Width="800">
    <Window.DataContext>
        <local:DataContextMainWindow/>
    </Window.DataContext>
    <Grid>
        <Label Name="StartTime" Content="{Binding Participant.Name}" HorizontalAlignment="Left" Margin="135,123,0,0" VerticalAlignment="Top"/>
        <Label Name="Points" Content="{Binding Points}" HorizontalAlignment="Left" Margin="135,96,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
